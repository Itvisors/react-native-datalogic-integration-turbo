{
  "name": "react-native-datalogic-integration-turbo",
  "version": "1.0.0",
  "description": "React Native DataLogic integration for New Architecture",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "react-native.config.js",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-datalogic-integration-turbo-example",
    "clean": "del-cli android/build example/android/build example/android/app/build lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "release": "release-it --only-version",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itvisors/react-native-datalogic-integration-turbo.git"
  },
  "author": "Marcel Groeneweg <mgroeneweg@users.noreply.github.com> (https://github.com/mgroeneweg)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itvisors/react-native-datalogic-integration-turbo/issues"
  },
  "homepage": "https://github.com/itvisors/react-native-datalogic-integration-turbo#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/react": "^19.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^2.8.8",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-builder-bob": "^0.40.18",
    "release-it": "^19.0.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "DatalogicIntegrationTurboSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.datalogicintegrationturbo"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 120
  },
  "create-react-native-library": {
    "type": "turbo-module",
    "languages": "kotlin-objc",
    "tools": [
      "release-it",
      "eslint"
    ],
    "version": "0.57.2"
  }
}
